#!/bin/zsh

wenv_setup() {
    WENV_DIR="/home/grish/scratch/webdev/angular-tour-of-heroes"
    WENV_ALIASES=()
    WENV_PROJECT='webdev'
    WENV_BRANCH='master'
    wenv_init() {
        wenv_tmux_layout 'devloop' 'yarn start'
        cd src/app
        kak app.{component,module}.ts hero{,.service}.ts {hero-detail,heroes}.component.ts
    }
}
