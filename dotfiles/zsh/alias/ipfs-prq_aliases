#!/bin/zsh

wenv_setup() {
    WENV_DIR="$GOPATH/src/github.com/ipfs/go-ipfs/exchange/bitswap/decision"
    WENV_ALIASES=()
    WENV_PROJECT='round-robin-prq'
    WENV_BRANCH='impl/bitswap/round-robin-prq'
    WENV_TASK=76
    wenv_init() {
        edit_project
        wenv_tmux_layout 'dev' 'wenv t'
    }
    wenv_shutdown() { }
}

edit_project() {
    [[ ! $(pwd) = "$WENV_DIR" ]] && wenv c
    kak peer_request_queue{_test,}.go strategy_prq.go
}
